{
  "name": "pngquant",
  "version": "2.0.0",
  "description": "The pngquant utility as a readable/writable stream",
  "main": "lib/PngQuant.js",
  "directories": {
    "test": "test"
  },
  "optionalDependencies": {
    "pngquant-bin": "^4.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.4",
    "mocha": "^3.3.0",
    "semver": "^5.3.0",
    "sinon": "^2.1.0",
    "unexpected": "^10.27.0",
    "unexpected-sinon": "^10.7.1",
    "unexpected-stream": "^2.1.0"
  },
  "scripts": {
